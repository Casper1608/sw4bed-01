{
  "version": "0.2.0",
  "configurations": [
    {
      "name": ".NET Core Launch (MTG Collection)",
      "type": "coreclr",
      "request": "launch",
      "preLaunchTask": "build",
      "program": "${workspaceFolder}/examples/lesson-12-persisting-data-with-mongo-db/mtg-collection/bin/Debug/net6.0/mtg-collection.dll",
      "args": [],
      "cwd": "${workspaceFolder}",
      "stopAtEntry": false,
      "console": "integratedTerminal"
    },
    {
      "name": ".NET Core Launch (WebAPIConfiguration)",
      "type": "coreclr",
      "request": "launch",
      "preLaunchTask": "build",
      "program": "${workspaceFolder}/examples/lesson-07-configuration/WebAPIConfiguration/bin/Debug/net6.0/WebAPIConfiguration.dll",
      "args": [],
      "cwd": "${workspaceFolder}",
      "stopAtEntry": false,
      "console": "integratedTerminal"
    },
    {
      "name": ".NET Core Launch (console)",
      "type": "coreclr",
      "request": "launch",
      "preLaunchTask": "build",
      "program": "${workspaceFolder}/examples/lesson-02-docker/multi-container/multi-container.migrations/bin/Debug/net6.0/multi-container.migrations.dll",
      "args": [],
      "cwd": "${workspaceFolder}",
      "stopAtEntry": false,
      "console": "internalConsole"
    },
    {
      "name": ".NET Core Launch (web)",
      "type": "coreclr",
      "request": "launch",
      "preLaunchTask": "build",
      "program": "${workspaceFolder}/examples/lesson11-improving-application-security/cors/endpoint/bin/Debug/net5.0/endpoint.dll",
      "args": [],
      "cwd": "${workspaceFolder}/examples/lesson11-improving-application-security/cors/endpoint",
      "stopAtEntry": false,
      "serverReadyAction": {
        "action": "openExternally",
        "pattern": "\\bNow listening on:\\s+(https?://\\S+)"
      },
      "env": {
        "ASPNETCORE_ENVIRONMENT": "Development"
      },
      "sourceFileMap": {
        "/Views": "${workspaceFolder}/Views"
      }
    },
    {
      "name": ".NET Core Attach",
      "type": "coreclr",
      "request": "attach"
    },
    {
      "name": "Docker .NET Launch",
      "type": "docker",
      "request": "launch",
      "preLaunchTask": "docker-run: debug",
      "netCore": {
        "appProject": "${workspaceFolder}/exercises/solution-proposals/lesson-10/Lists/src/Lists/Lists.csproj"
      }
    }
  ]
}